<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:s="/struts-tags"
	xmlns:wiki="/oba-wiki-tags">
<jsp:directive.page contentType="text/html" />
<head>
<title>OneBusAway NYC: <s:property value="name" /></title>
</head>
<body>
<s:if test="hasToc">
	<div id="content" class="sidebar">
		<div id="sidebar">
			<div class="wrapper">
				<s:property value="toc" escape="false"/>
			</div>
		</div>

		<div id="text">
			<s:property value="content" escape="false"/>

			<s:if test="admin">
			  <p class="edit"><s:a href="%{editLink}">Edit This Page</s:a></p>
			</s:if>
		</div>
	</div>
</s:if>
<s:else>
	<div id="content">
		<div id="text">
			<s:property value="content" escape="false"/>

			<s:if test="admin">
			  <p class="edit"><s:a href="%{editLink}">Edit This Page</s:a></p>
			</s:if>
		</div>
	</div>
</s:else>
</body>
</html>