<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:s="/struts-tags"
	xmlns:wiki="/oba-wiki-tags">
<jsp:directive.page contentType="text/html" />
<head>
<title>One Bus Away NY Admin Interface</title>
<link rel="stylesheet" href="/onebusaway-nyc-webapp/css/reporting.css" type="text/css" media="screen,print"/>
</head>
<body>
<div id="text">
<h1>Generate Report</h1>
<s:if test="%{reportError != null}">
<p class="error"><s:property value="reportError" /></p>
</s:if>
<s:form method="GET" action="reporting!submit">
<div><s:textarea name="query"></s:textarea></div>
<div><s:submit value="Download CSV" /></div>
</s:form>
</div>
</body>
</html>
