<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:s="/struts-tags"
	xmlns:wiki="/oba-wiki-tags">
<jsp:directive.page contentType="text/html" />
<head>
<title>One Bus Away NY</title>
</head>
<body>

<s:form method="post" action="edit-parameters!submit">
  <s:textfield name="noProgressTimeout" value="%{noProgressTimeout}" label="Timeout for No Progress"/>
  <s:textfield name="offRouteDistance" value="%{offRouteDistance}" label="Off Route Distance"/>
  <s:textfield name="staleDataTimeout" value="%{staleDataTimeout}" label="Stale Data Timeout"/>
  <s:submit name="submit" value="Submit"/>
</s:form>

</body>
</html>
